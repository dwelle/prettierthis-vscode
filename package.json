{
    "name": "prettierthis",
    "displayName": "PrettierThis",
    "author": "Nir Adler",
    "publisher":"niradler",
    "description": "run prettier on the current selection.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.prettierthis"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.prettierthis",
                "title": "Prettier This"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.prettierthis",
                    "group": "YourGroup@1"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "prettier": "1.15.3",
        "typescript": "^3.1.4",
        "vscode": "^1.1.25"
    }
}
