{
    "name": "prettierthis",
    "displayName": "PrettierThis",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/niradler/prettierthis-vscode"
    },
    "keywords": [
        "prettier",
        "vscode"
    ],
    "author": "Nir Adler",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/niradler/prettierthis-vscode/issues"
    },
    "homepage": "https://niradler.com",
    "publisher": "niradler",
    "description": "Run prettier on the current selection.",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.prettierthis"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.prettierthis",
                "title": "Prettier This"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.prettierthis",
                    "group": "YourGroup@1"
                }
            ]
        }
    },
    "scripts": {
        "create-publisher":"vsce create-publisher niradler",
        "publish":"vsce publish",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "echo \"no tests\"",
        "test-vscode": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "prettier": "^1.15.3"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "prettier": "^1.15.3",
        "typescript": "^3.1.4",
        "vscode": "^1.1.25"
    }
}
